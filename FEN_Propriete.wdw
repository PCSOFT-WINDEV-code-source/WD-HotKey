  -
    name : BTNOK
    identifier : 0x2924338128d8dcd7
    internal_properties : 
    properties :
     x : 287
     y : 171
     width : 75
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAACVSnGBPDXMsWj6x6OLCmZy/7eDd8RNuL9gY1opYk+TQGdZYvQcaOnQ9ZRtNuXUCB3S82anRncrbDjh9QlpuZfM/ha251TmIhv9BUk654iI6G1XmVyhMBl05cXw/21tO13gIyuQuXCtzh/3xNurRFNVJwoHjRrIjYOX/yYcflu1IPCnKFxnPHw93cL+gWgvDHQEjVi1wDR2ja0RFg3gnNY=
        code : |1-
         // Si rien n'a été modifié, fermeture de la fenêtre
         si FEN_Propriété..modifie = faux ALORS
         	ferme("",faux)
         FIN
         
         /////////////// Contrôle de saisie  ////////////////
         // Au moins une touche de contrôle
         si (INT_CTRL[1] ou INT_CTRL[2] ou INT_CTRL[3]) = faux alors
         	erreur(<§$0001§>)
         	retour
         FIN
         
         // cible
         si SAI_CIBLE~="" alors
         	erreur(<§$0002§>)
         	retour
         FIN
         
         // description
         si SAI_DESC~="" alors
         	si pas confirmer(<§$0003§>,<§$0004§>) retour
         FIN
         
         // Ajout ?
         si nIDAuto=0 alors
         	// Doublon de hotkey ?
         	HLitRecherche(HOTKEY, Combinaison, [INT_CTRL[1], INT_CTRL[2], INT_CTRL[3]], hIdentique)
         	si htrouve() alors
         		si ouinon(<§$0005§>,<§$0006§>)=non
         			retour
         		sinon
         			hsupprime(HOTKEY)
         		FIN
         	fIN
         FIN
         
         /////////////// Ajout/Modification /////////////// 
         HOTKEY.Ctrl		= INT_CTRL[1]
         HOTKEY.alt		= INT_CTRL[2]
         HOTKEY.shift		= INT_CTRL[3]
         HOTKEY.Cible		= SAI_CIBLE
         HOTKEY.Description	= SAI_DESC
         HOTKEY.Touche		= sansespace(COMBO_TOUCHE[COMBO_TOUCHE])
         
         // Ajout ?
         si nIDAuto=0 alors
         	hajoute(HOTKEY)
         	
         // Modif ?
         sinon
         	HModifie(HOTKEY)
         FIN
         
         // Suppression du fichier temporaire
         fsupprime(fRépertoireTemp()+"IcoTemp.bmp")
         
         ferme(FEN_Propriété,vrai)
        type : 18
