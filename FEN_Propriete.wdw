     code : |1+
      procedure FEN_Propriété(nIDAuto=0)
   -
     code : |1+
      si nIDAuto>0 alors
      	HLitRecherchepremier(HOTKEY,IDHOTKEY,nIDAuto)
      	si htrouve() alors
      		INT_CTRL[1] 	= HOTKEY.Ctrl
      		INT_CTRL[2] 	= HOTKEY.alt
      		INT_CTRL[3]	= HOTKEY.shift
      		SAI_CIBLE		= HOTKEY.Cible
      		SAI_DESC		= HOTKEY.Description
      		nIndice est un entier
      		nIndice = ListeCherche(COMBO_TOUCHE, HOTKEY.Touche)
      		si nIndice<>-1 alors
      			ListeSelectPlus(COMBO_TOUCHE, nIndice)
      		fin
      		
      	sinon
      		erreur(<§$0000§>)
      		ferme()
      	FIN
      FIN
